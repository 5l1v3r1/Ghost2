call pathogen#infect()
call pathogen#helptags()

syntax on
set background=dark
colorscheme monokai

set tabstop=4
set softtabstop=4 

filetype plugin indent on 
let delimitMate_expand_cr = 1
set autoindent
set copyindent
set shiftwidth=4
set smarttab

set cursorline
set number

set list
set listchars=tab:▸\ ,eol:¬


